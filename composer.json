{
    "name": "eacg-gmbh/ecs-composer",
    "description": "Module for Composer allowing the transfer of package dependencies to ECS for further legal and vulnerability analysis. See https://ecs.eacg.de for a detailed service description.",
    "keywords": ["composer", "package", "plugin", "opensource", "dependency-analysis"],
    "license": "MIT",
    "authors": [
        {
            "name": "Anatolii Varanytsia",
            "email": "prizrack13@mail.ru"
        }
    ],
    "support": {
        "email": "ecs-support@eacg.de"
    },
    "type": "library",
    "version": "1.0.0",
    "bin": ["bin/ecs-composer"],
    "require": {
		"php": ">=5.4",
        "composer/composer": "^1.4",
        "curl/curl": "^1.6",
        "league/climate": "^3.2"
    },
	"require-dev": {
		"phpunit/phpunit": "5.2.*"
    },
	"autoload": {
        "psr-4": {
            "EacgGmbh\\ECSComposer\\": "src/"
        }
    }
}
